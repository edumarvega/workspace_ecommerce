<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	
    <bean id="homeAction" class="ar.com.templateit.ecommerce.action.HomeAction" scope="prototype">
		<property name="sendSMSSoap" ref="sendSMSSoap" />	
		<property name="service1Soap" ref="service1Soap" />
	</bean>
	
    <bean id="productoAction" class="ar.com.templateit.ecommerce.action.ProductoAction" scope="prototype">
			<property name="service1Soap" ref="service1Soap" />
	</bean>
	
    <bean id="pedidoAction" class="ar.com.templateit.ecommerce.action.PedidoAction" scope="prototype">
			<property name="service1Soap" ref="service1Soap" />
	</bean>
 
  
</beans>
